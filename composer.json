{
    "name": "open-itcockpit/TelegramModule",
    "description": "TelegramModule plugin for openITCOCKPIT",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0",
        "telegram-bot/api": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TelegramModule\\": "src/",
            "TelegramBot\\Api\\": "vendor/telegram-bot/api/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TelegramModule\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
